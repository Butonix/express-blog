{
  "name": "express-blog-unit",
  "version": "1.0.0",
  "description": "Bridge connecting frontend and backend for Smiler.",
  "scripts": {
    "run:dev": "concurrently --kill-others \"cd server && npm start\" \"cd client && npm run serve\"",
    "front:build": "cd client && npm run build",
    "front:serve": "cd client && npm run serve",
    "back:serve": "cd server && npm start",
    "run:production": "cd server && node index.js"
  },
